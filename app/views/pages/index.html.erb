            <li class="icon">
              <%= link_to image_tag("twitter.svg"), "https://twitter.com/ekluff" %>
            </li>
            <li class="icon">
              <%= link_to image_tag("linkedin.svg"), "https://linkedin.com/in/eclough" %>
