    <div class="fus-overlay"></div>
    <div class="container">
      <div class="fus-pitch">
        <div class="fus-call-to-action">
          <h1>Evan Clough</h1>
          <h2>adventure photography</h2>
          <ul class="list social-icons list-inline list-center">
            <li class="icon">
              <%= link_to image_tag("instagram.svg"), "https://instagram.com/ekluff" %>
