<div class="fus-overlay" id="images"></div>

<div class="row">
  <div class="slider">
    <ul class="list list-unstyled list-inline img-list slider-list">
      <% @images.each do |image| %>
        <li class="img img-md img-slider"><%= link_to image_tag(image.asset.url(:medium)), image %></li>
      <% end %>
    </ul>
  </div>
</div>
